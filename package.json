{
  "version": "1.0.9",
  "description": "A simple animated waving flag from a jpg/png image",
  "keywords": [
    "component",
    "animated",
    "waving",
    "flag",
    "react",
    "react-component",
    "reactjs",
    "typescript",
    "visualization"
  ],
  "homepage": "https://github.com/garvae/react-waving-flag",
  "funding": {
    "url": "https://ko-fi.com/garvae"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garvae/react-waving-flag"
  },
  "bugs": {
    "url": "https://github.com/garvae/react-waving-flag/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.js",
  "module": "dist/react-waving-flag.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "size-limit",
    "build": "yarn rollup:compile",
    "copy:license": "cpx LICENSE dist",
    "copy:readme": "cpx README.md dist",
    "delete:dist": "rimraf dist",
    "delete:sourcemaps": "del dist/**/*.js.map",
    "generate:packagejosn": "node generate-package-json.js",
    "generate:types": "tsc dist/index.js --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "lint:js": "cross-env LINT=true eslint src/**/*.{ts,tsx} --fix",
    "postbuild": "yarn generate:packagejosn && yarn copy:license && yarn generate:types && yarn copy:readme",
    "postprepare": "yarn generate:packagejosn",
    "prebuild": "yarn delete:dist && yarn lint:js",
    "publish:dist": "cd dist && npm publish",
    "rollup:compile": "rollup src/index.ts --file dist/index.js --format cjs -c rollup.config.js",
    "size": "size-limit",
    "start": "tsdx watch",
    "version:patch": "npm version patch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env LINT=true eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,json,jsx,ts,tsx,css,scss,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@garvae/react-waving-flag",
  "author": "Vova Garvae",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@getify/eslint-plugin-proper-arrows": "^11.0.3",
    "@getify/eslint-plugin-proper-ternary": "^3.1.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "autoprefixer": "^10.4.4",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.5",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-boundaries": "^2.8.0",
    "eslint-plugin-destructuring-newline": "^0.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-newline-destructuring": "^1.0.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.1-rc.0-next-848e802d2-20220204",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jsdoc": "^3.6.10",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "postcss": "^8.4.12",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.67.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
